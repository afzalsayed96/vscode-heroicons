{
  "name": "heroicons",
	"displayName": "Hero Icons",
	"publisher": "afzalsayed96",
	"author": "Afzal Sayed",
	"repository": {
		"url": "https://github.com/afzalsayed96/vscode-heroicons"
	},
	"icon": "media/icon.png",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:heroicons.heroicons"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "heroicons.heroicons",
				"title": "Hero Icons"
			}
		]
